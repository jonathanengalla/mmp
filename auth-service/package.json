{
  "name": "auth-service",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "main": "dist/auth-service/src/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "prisma generate && tsc -b",
    "start": "node dist/auth-service/src/server.js",
    "test": "echo \"No tests configured for auth-service\"",
    "test:attendance": "node -r ts-node/register --test tests/attendanceHandlers.test.ts",
    "test:event-invoices": "node -r ts-node/register --test tests/eventInvoiceHandlers.test.ts",
    "test:evt01-schema": "node -r ts-node/register --test tests/evt01-schema-guard.test.ts",
    "test:evt02-registration": "node -r ts-node/register --test tests/evt02-registration-flow.test.ts",
    "test:fin-01": "node -r ts-node/register --test tests/financeSummary.test.ts",
    "test:fin02": "node -r ts-node/register --test tests/fin02-invoice-list.test.ts",
    "test:bks05-events-billing": "node -r ts-node/register --test tests/bks05-events-billing-linkage.test.ts",
    "test:pay10-status": "node -r ts-node/register --test tests/invoiceStatusEngine.test.ts",
    "test:pay10-balance": "node -r ts-node/register --test tests/invoiceBalance.test.ts",
    "test:pay10-regression": "node -r ts-node/register --test tests/fin01-fin02-regression.test.ts",
    "migrate:dev": "prisma migrate dev --schema prisma/schema.prisma --skip-seed",
    "migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "debug:prisma-engines": "node -e \"console.log(require.resolve('@prisma/engines'))\"",
    "db:migrate": "NODE_ENV=production node scripts/db-migrate.js",
    "db:seed:base": "NODE_ENV=production node scripts/db-seed-base.js",
    "db:reset:test": "NODE_ENV=test node scripts/db-reset-and-seed-test.js",
    "demo:reset": "node scripts/demo-reset-and-seed.js",
    "hash:dev-password": "npm run build && node scripts/hash-dev-password.js",
    "seed:rcme": "ts-node --transpile-only scripts/seed-rcme-sandbox.ts",
    "seed:rcme:reset": "ts-node --transpile-only scripts/seed-rcme-sandbox.ts --reset",
    "migrate:pay10-backfill": "ts-node --transpile-only scripts/migrate-pay10-backfill.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@prisma/engines": "5.22.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "jsonwebtoken": "^9.0.3",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
