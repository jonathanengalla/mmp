{
  "name": "auth-service",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "main": "dist/auth-service/src/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "npx prisma@5.22.0 generate && tsc -b",
    "start": "node dist/auth-service/src/server.js",
    "test": "echo \"No tests configured for auth-service\"",
    "db:migrate": "NODE_ENV=production node scripts/db-migrate.js",
    "db:seed:base": "NODE_ENV=production node scripts/db-seed-base.js",
    "db:reset:test": "NODE_ENV=test node scripts/db-reset-and-seed-test.js",
    "demo:reset": "node scripts/demo-reset-and-seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "jsonwebtoken": "^9.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "prisma": "^5.22.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
